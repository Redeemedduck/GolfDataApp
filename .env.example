# Supabase Configuration
SUPABASE_URL=https://xxxxx.supabase.co
SUPABASE_KEY=your-anon-key-here

# AI Service Keys (Local Analysis Only)
GEMINI_API_KEY=AIza...
ANTHROPIC_API_KEY=sk-ant-api...

# Google Cloud Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1
BQ_DATASET_ID=golf_data
BQ_TABLE_ID=shots

# Optional: Path to service account JSON (if not using gcloud auth)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# ============================================
# AUTOMATION CONFIGURATION
# ============================================

# Uneekor Portal Credentials (for automated login)
# If not set, you'll need to run 'python automation_runner.py login' once
# to save cookies for future automated runs
UNEEKOR_USERNAME=your_email@example.com
UNEEKOR_PASSWORD=your_password_here

# Cookie encryption key (auto-generated if not set)
# UNEEKOR_COOKIE_KEY=your-32-byte-key-here

# Slack Notifications
# Get webhook URL from: https://api.slack.com/apps -> Incoming Webhooks
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/T00000000/B00000000/XXXXXXXXXXXXXXXXXXXXXXXX
# SLACK_CHANNEL=#golf-data  # Optional: override default channel
# SLACK_USERNAME=GolfDataApp Bot  # Optional: custom bot name

# Notification Settings
NOTIFICATION_CONSOLE=true
NOTIFICATION_LOG=true

# Rate Limiting (be conservative to avoid being blocked)
# Default: 6 requests per minute (one every 10 seconds)
# AUTOMATION_RATE_LIMIT_RPM=6

# Backfill Settings
# BACKFILL_MAX_SESSIONS_PER_RUN=50
# BACKFILL_CHECKPOINT_INTERVAL=5
